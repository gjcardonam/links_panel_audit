          "Benchmark",
          "Daily ESP Status",
          "Executive",
          "Health Check",
          "Production History",
          "Production Optimization",
          "Real Time Multi-axis",
          "Real Time: Quick View",
          "Real Time: Trends",
          "Reports",
          "Setpoints",
          "Shutdowns and Faults"
        ],
        "names_filter": [
          "Executive",
          "Benchmark",
          "Production Optimization",
          "Status",
          "Shutdowns",
          "Production",
          "Single-Axis",
          "Multi-axis",
